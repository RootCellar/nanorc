syntax "dotenv" "\.env" "\.env\..+"

# Keys
color brightred "^[^=]+"

# Values
color brightcyan "[^=]+$"

# Erroneous Spaces
color ,red "[[:space:]]"

# Operators
color brightblack "(\(|\)|\$|=)"

# Strings
color brightgreen ""(\\.|[^"])*"" "'(\\.|[^'])*'"

# Comments
color brightblue "(^|[[:space:]])#.*$"

# Whitespace
color ,green "[[:space:]]+$"
